extends layouts/main
include mixins/videoBlock

block content  
    .search__header
        h3 Searching for #{searchingBy}
    .search__header    
         each item in videos
            //- h1= item.title
            //- p= item.description
            +videoBlock({
                title:item.title,
                views:item.views,
                videoFile: item.videoFile

            })